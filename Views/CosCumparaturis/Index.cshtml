@model IEnumerable<ShoesStore.Models.CosCumparaturi>

@{
    ViewData["Title"] = "Index";
}

<link rel="stylesheet" href="~/css/home.css" />

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Cantitate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Id_Produs)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Cantitate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Id_Produs)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id_Cos">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id_Cos">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id_Cos">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
